<h1>Listing daily_values</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Day time</th>
    <th>Weight</th>
  </tr>

<% @daily_values.each do |daily_values| %>
  <tr>
    <td><%=h daily_values.id %></td>
    <td><%=h daily_values.day_time %></td>
    <td><%=h daily_values.weight %></td>
    <td><%= link_to 'Show', daily_values %></td>
    <td><%= link_to 'Edit', edit_daily_values_path(daily_values) %></td>
    <td><%= link_to 'Destroy', daily_values, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New daily_values', new_daily_values_path %>